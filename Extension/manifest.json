{
    "manifest_version": 3,
    "name": "Gestion de compte",
    "description": "",
    "version": "1.0",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "100": "images/icon100.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/icon150.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}